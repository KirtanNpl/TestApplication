@model IEnumerable<Infrastructure.Entity.QualificationList>

@{
    ViewData["Title"] = "Index";
}

<h1>List </h1>

<p>
    <a asp-action="SaveQualification">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Q_Name)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Q_Name)
            </td>
            <td>
                @Html.ActionLink("Edit", "UpdateQualification", new { id=item.Id }) |
               
                @Html.ActionLink("Delete", "DeleteQualification", new {  id=item.Id })
            </td>
        </tr>
}
    </tbody>
</table>
